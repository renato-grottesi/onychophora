; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="onychophora"
run/main_scene="res://ui/MainScreen.tscn"
boot_splash/fullsize=false
config/icon="res://icon.png"

[autoload]

global="*res://global.gd"

[debug]

settings/fps/force_fps=12

[display]

window/size/width=64
window/size/height=64
window/size/fullscreen=true
window/stretch/mode="2d"
window/stretch/aspect="keep"
window/stretch/shrink="1"

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/2d/use_pixel_snap=true
quality/filters/anisotropic_filter_level=1
quality/depth_prepass/enable=false
quality/voxel_cone_tracing/high_quality=false
environment/default_clear_color=Color( 0, 0, 0, 1 )
quality/depth/hdr=false
environment/default_environment="res://default_env.tres"
